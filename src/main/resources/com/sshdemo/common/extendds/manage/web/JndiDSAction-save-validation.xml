<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE validators PUBLIC "-//OpenSymphony Group//XWork Validator 1.0.2//EN"
"http://struts.apache.org/dtds/xwork-validator-1.0.2.dtd">

<validators>
	<field name="jndiDSVo.name">
		<field-validator type="requiredstring">
			<message>名称不能为空</message>
		</field-validator>
		<field-validator type="stringlength">
			<param name="maxLength">10</param>
			<message>名称不能大于${maxLength}个字符</message>
		</field-validator>
	</field>
	<field name="jndiDSVo.jndiName">
		<field-validator type="requiredstring">
			<message>Jndi名不能为空</message>
		</field-validator>
		<field-validator type="stringlength">
			<param name="maxLength">40</param>
			<message>Jndi名不能大于${maxLength}字符</message>
		</field-validator>
	</field>
</validators>